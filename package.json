{
  "name": "ngx-f2",
  "version": "1.0.0",
  "main": "./bundles/f2.umd.js",
  "module": "./esm5/f2.js",
  "es2015": "./esm2015/f2.js",
  "typings": "./f2.d.ts",
  "description": "Angular2+ wrapper for Antv F2 mobile charts library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hamdiwanis/ngx-f2.git"
  },
  "keywords": [
    "ngx-f2",
    "angular f2",
    "angular2 f2",
    "angular4 f2",
    "angular5 f2",
    "angular6 f2",
    "angular7 f2",
    "ngx f2",
    "f2"
  ],
  "author": "hamdiwanis <hamdiwanis>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hamdiwanis/ngx-f2/issues"
  },
  "homepage": "https://github.com/hamdiwanis/ngx-f2#readme",
  "scripts": {
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "tslint -p tsconfig.json -c tslint.json 'lib/*/*.ts'",
    "test": "ng test --code-coverage",
    "site:build": "ng build --prod --build-optimizer",
    "site:gh": "ng build --prod --build-optimizer --base-href /ngx-f2/ && gh-pages -d ./dist",
    "build": "bash ./build.sh",
    "release:next": "bash ./build.sh && cd publish && npm publish --access public --tag next",
    "release": "bash ./build.sh && cd publish && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^7.1.1",
    "@antv/f2": "^3.3.1",
    "core-js": "^2.5.7",
    "ngx-highlight-js": "^2.1.1",
    "rxjs": "^6.3.3",
    "tsickle": "^0.34.0",
    "zone.js": "^0.8.26",
    "ngx-f2": "1.0.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codecov": "^3.0.0",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "rollup": "^0.67.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.1"
  }
